{
  "name": "@emacs-eask/eask",
  "version": "0.6.15",
  "description": "A set of command-line tools to build Emacs packages",
  "main": "eask",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg package.json"
  },
  "bin": {
    "eask": "./eask"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emacs-eask/eask.git"
  },
  "keywords": [
    "emacs",
    "package",
    "management",
    "cli"
  ],
  "author": "Jen-Chieh Shen",
  "license": "GPL-3.0",
  "dependencies": {
    "yargs": "^17.0.0"
  },
  "files": [
    "cmds",
    "src",
    "lisp"
  ],
  "publishConfig": {
    "access": "public"
  },
  "pkg": {
    "scripts": [
      "cmds/**/*.js",
      "src/**/*.js"
    ],
    "assets": [
      "lisp/**/*.el"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  }
}
